{
  "name": "@awboost/cfntypes",
  "version": "1.0.0-beta.52",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "author": {
    "email": "gordonmleigh@gmail.com",
    "name": "Gordon Leigh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awboost/cfntypes.git"
  },
  "homepage": "https://github.com/awboost/cfntypes#readme",
  "license": "MIT",
  "exports": {
    "./definitions": {
      "types": "./lib/definitions.d.ts",
      "default": "./lib/definitions.js"
    },
    "./policies": {
      "types": "./lib/policies.d.ts",
      "default": "./lib/policies.js"
    },
    "./resources": {
      "types": "./lib/resources.generated.d.ts",
      "default": "./lib/resources.generated.js"
    },
    "./template": {
      "types": "./lib/template.d.ts",
      "default": "./lib/template.js"
    },
    "./util": {
      "types": "./lib/util.d.ts",
      "default": "./lib/util.js"
    }
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.map",
    "!lib/**/*.test.*"
  ],
  "scripts": {
    "lint": "eslint src/ --ext=ts"
  },
  "awsResourceSpecificationVersion": "140.0.0"
}