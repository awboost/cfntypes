{
  "name": "@awboost/cfntypes",
  "version": "1.0.0-beta.60",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "author": {
    "email": "gordonmleigh@gmail.com",
    "name": "Gordon Leigh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awboost/cfntypes.git"
  },
  "homepage": "https://github.com/awboost/cfntypes#readme",
  "license": "MIT",
  "exports": {
    "./composition": {
      "types": "./lib/composition.d.ts",
      "default": "./lib/composition.js"
    },
    "./intrinsics": {
      "types": "./lib/intrinsics.d.ts",
      "default": "./lib/intrinsics.js"
    },
    "./resources/*": {
      "types": "./lib/resources.generated/*.d.ts",
      "default": "./lib/resources.generated/*.js"
    },
    "./template": {
      "types": "./lib/template.d.ts",
      "default": "./lib/template.js"
    }
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.map",
    "!lib/**/*.test.*"
  ],
  "scripts": {
    "lint": "eslint src/ --ext=ts"
  },
  "awsResourceSpecificationVersion": "143.0.0"
}